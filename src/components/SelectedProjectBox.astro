---
import SelectedProjectItem from './SelectedProjectItem.astro';
import type { SelectedProject } from '@/data';

interface Props {
  projects: SelectedProject[];
}

const { projects = [] } = Astro.props;
---

<div class="flex flex-col p-1">
  {projects.map((project) => <SelectedProjectItem project={project} />)}
</div>
