---
import WorkExperienceItem from './WorkExperienceItem.astro';
import type { WorkExperience } from '@/data';

interface Props {
  workExperiences: WorkExperience[];
}

const { workExperiences = [] } = Astro.props;
---

<div class="flex flex-col p-1">
  {workExperiences.map((workExperience) => <WorkExperienceItem workExperience={workExperience} />)}
</div>
